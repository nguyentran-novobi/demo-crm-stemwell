<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">
    <record id="ir_cron_get_ringcentral_call_log" model="ir.cron">
        <field name="name">NERP RingCentral: Get Call Log</field>
        <field name="model_id" ref="model_ringcentral_call_log"/>
        <field name="state">code</field>
        <field name="code">model.get_ringcentral_call_log()</field>
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False" />
        <field name="active" eval="True" />
    </record>
</odoo>